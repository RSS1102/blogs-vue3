{
    "name": "vue3-ts",
    "version": "0.0.0",
    "license": "ISC",
    "scripts": {
        "dev": "vite --mode development",
        "build": "vue-tsc --noEmit && vite build --mode development",
        "build:production": "vue-tsc --noEmit && vite build --mode production",
        "build:gitpages": "vue-tsc --noEmit && vite build --mode gitpages"
    },
    "dependencies": {
        "@babel/types": "^7.20.5",
        "@element-plus/icons-vue": "^2.0.10",
        "@vitejs/plugin-vue": "^3.2.0",
        "axios": "^1.2.1",
        "element-plus": "^2.2.26",
        "less": "^4.1.3",
        "qs": "^6.11.0",
        "vue": "^3.2.45",
        "vue-router": "^4.1.6"
    },
    "devDependencies": {
        "@types/node": "^18.11.11",
        "@types/qs": "^6.9.7",
        "typescript": "^4.9.3",
        "vite": "^3.2.5",
        "vue-tsc": "^1.0.11"
    },
    "babel": {
        "plugins": [
            [
                "component",
                {
                    "libraryName": "element-ui",
                    "styleLibraryName": "theme-chalk"
                }
            ]
        ]
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/RSS1102/web.git"
    }
}
